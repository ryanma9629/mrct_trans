# API Keys for different LLM providers
# You only need to set the keys for the providers you plan to use

# OpenAI API Key for ChatGPT
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_DEFAULT_MODEL=gpt-4o-mini

# Azure OpenAI Configuration
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com/
AZURE_OPENAI_API_VERSION=2025-03-01-preview
AZURE_OPENAI_DEFAULT_MODEL=gpt-4o-mini

# DeepSeek API Configuration
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_ENDPOINT=https://api.deepseek.com/chat/completions
DEEPSEEK_DEFAULT_MODEL=deepseek-chat

# Qwen API Configuration (Alibaba Cloud DashScope)
# DASHSCOPE_API_KEY=your_alternate_dashscope_api_key_here
DASHSCOPE_API_KEY=your_dashscope_api_key_here
QWEN_ENDPOINT=https://dashscope.aliyuncs.com/compatible-mode/v1/
QWEN_DEFAULT_MODEL=qwen-turbo